FUNCTION "SplitCommaString" (INPUT_STRING NVARCHAR(500000))
	RETURNS TABLE ("RESULT" NVARCHAR(255))
    LANGUAGE SQLSCRIPT
    SQL SECURITY INVOKER AS
BEGIN
	USING SQLSCRIPT_STRING AS LIBRARY;

	RETURN SELECT DISTINCT "RESULT" 
	FROM LIBRARY:SPLIT_TO_TABLE(:INPUT_STRING,',') 
	WHERE "RESULT" IS NOT NULL AND "RESULT" != '';
END